{
  "name": "googletester",
  "config": {
    "chromeVersion": "",
    "chromeVersionPrefix": ""
  },
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setChromeVersionForCI": "npm config set protractor-typescript-cucumber:chromeVersion $(node chromeVersion.js) && npm config set protractor-typescript-cucumber:chromeVersionPrefix versions.chrome",
    "webDriverStart": "./node_modules/.bin/cross-var ./node_modules/.bin/webdriver-manager start --detach --$npm_package_config_chromeVersionPrefix $npm_package_config_chromeVersion",
    "webDriverUpdate": "./node_modules/.bin/cross-var ./node_modules/.bin/webdriver-manager update --$npm_package_config_chromeVersionPrefix $npm_package_config_chromeVersion",
    "webDriverClean": "./node_modules/.bin/webdriver-manager clean",
    "init-contributors": "all-contributors init",
    "add-contributors": "all-contributors add",
    "build": "tsc",
    "clean": "rimraf typeScript/",
    "clean-build": "npm run clean && npm run build",
    "compileTs": "./node_modules/.bin/tsc -p .",
    "compile": "npm run clean && npm run compileTs",
    "seleniumKill": "./node_modules/.bin/fkill :4444 -s",
    "generate-contributors": "all-contributors generate",
    "testLocal": " npm test",
    "pretest": "npm run compile && npm run seleniumKill && npm run webDriverUpdate && npm run webDriverStart",
    "test": "./node_modules/.bin/cross-var ./node_modules/.bin/protractor cucumber.conf.js",
    "posttest": " npm run seleniumKill"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.2",
    "@types/cucumber": "^4.0.7",
    "@types/jquery": "^3.3.31",
    "@types/node": "^12.7.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-var": "^1.1.0",
    "cucumber": "^5.0.2",
    "cucumber-html-reporter": "^4.0.3",
    "cucumber-junit": "^1.7.1",
    "find-chrome-version": "^1.1.0",
    "fkill-cli": "^5.2.0",
    "protractor": "^5.4.4",
    "protractor-cucumber-framework": "^6.1.3",
    "protractor-cucumber-junit": "^1.1.3",
    "rimraf": "^2.7.1",
    "tslib": "^1.10.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "typescript-tslint-plugin": "^0.5.4"
  }
}
